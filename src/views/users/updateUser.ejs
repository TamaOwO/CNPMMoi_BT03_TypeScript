<div class="container">
    <div class="row"
        <form action="/put-crud" method="post">
            <input type="text" class="form-control" id="id" name="id" value="<%= user.id %>"

            <div class="form-row">
                <div class="form-group col-md-6">
                    <label for="inputEmail4">First Name:</label>
                    <input type="text" class="form-control" id="firstName" id="firstName" value="<%= data.firstName%>">
                </div>
                <div class="form-group col-md-6">
                    <label for="inputPassword4">Last Name:</label>
                    <input type="text" class="form-control" id="lastName" id="lastName" value="<%= data.lastName%>">
                </div>
            </div>
            <div class="form-row">
                    <label for="inputEmail4">Address</label>
                    <input type="text" class="form-control" id="address" placceholder="179A" name ="address"
                    value="<%= data.address%>">
            </div>

            <button type="submit" class="btn btn-primary">Update</button>
        </form>
    </div>
</div>
            